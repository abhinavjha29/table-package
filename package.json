{
  "name": "react-table-extension",
  "version": "1.0.3",
  "author": "Abhinav Jha",
  "description": "Create tables with nested react components.",
  "keywords": [
    "remirror",
    "preset",
    "react",
    "tables",
    "prosemirror",
    "table editing"
  ],
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "files": [
    "dist",
    "dist-types"
  ],
  "scripts": {
    "build": "remirror-cli build"
  },
  "dependencies": {
    "@babel/runtime": "^7.22.3",
    "@emotion/css": "^11.11.0",
    "@linaria/core": "4.2.10",
    "@remirror/core": "^3.0.1",
    "@remirror/core-utils": "3.0.0",
    "@remirror/extension-positioner": "3.0.1",
    "@remirror/extension-tables": "3.0.1",
    "@remirror/icons": "3.0.0",
    "@remirror/messages": "3.0.0",
    "@remirror/react-components": "3.0.1",
    "@remirror/react-core": "3.0.1",
    "@remirror/react-hooks": "3.0.1",
    "@remirror/theme": "3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.7",
    "@babel/preset-env": "^7.26.7",
    "@babel/preset-react": "^7.26.3",
    "@remirror/cli": "^1.0.8",
    "@remirror/pm": "3.0.0",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.0.0",
    "rollup-plugin-babel": "^4.4.0",
    "y-webrtc": "^10.2.5",
    "yjs": "^13.6.1"
  },
  "peerDependencies": {
    "@remirror/pm": "^3.0.0",
    "react": "^16.14.0 || ^17 || ^18",
    "react-dom": "^16.14.0 || ^17 || ^18"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.esm.js",
        "require": "./dist/index.cjs.js"
      },
      "./package.json": "./package.json"
    },
    "main": "./dist/index.cjs.js",
    "module": "./dist/index.esm.js",
    "type": "module",
    "types": "./dist/index.d.ts"
  },
  "@remirror": {
    "sizeLimit": "20 KB"
  }
}
